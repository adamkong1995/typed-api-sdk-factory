{
  "name": "@adamkong1995/sdk-a",
  "version": "0.2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist src/gen",
    "generate:sdk": "pnpm openapi:gen",
    "openapi:gen": "openapi-ts",
    "build": "tsup src/index.ts --format esm --dts",
    "check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.13.4"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.91.1",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
