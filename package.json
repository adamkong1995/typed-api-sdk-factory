{
  "name": "typed-api-sdk-factory",
  "private": true,
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "check": "pnpm -r check",
    "generate:openapi": "pnpm -r generate:openapi",
    "generate:sdk": "pnpm -r generate:sdk",
    "ci:verify-generated": "pnpm generate:openapi && pnpm generate:sdk && git diff --exit-code",
    "clean": "pnpm -r clean || true",
    "demo": "pnpm generate:openapi && pnpm generate:sdk && pnpm -r --parallel dev",
    "build": "pnpm -r --filter \"@adamkong1995/sdk-*\" build"
  }
}
